<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Shape -->
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="0dp" /> <!-- Adjust the corner radius as needed -->
            <solid android:color="#FE6637" /> <!-- Set the fill color -->
            <stroke
                android:width="3dp"
                android:color="#000000" /> <!-- Set the stroke width and color -->
        </shape>
    </item>
</layer-list>
